<?php

function getPubs() {

  $pubListSrc = "sites/all/modules/wri_external_pub_list/wriauthors.json";

  $pubList = file_get_contents($pubListSrc);

  $itemArray = json_decode($pubList, true);

  $extPub = $itemArray;

  $test = $extPub[0]['title'];

  return $test;

}

$message = getPubs();
drupal_set_message(t($message), 'status');

